<UserControl
    x:Class="MithgardWpf.App.Core.Navigation.Views.PageHost"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ap="clr-namespace:MithgardWpf.App.Common.AttachedProperties"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:MithgardWpf.App.Core.Navigation.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Grid>
        <Frame
            x:Name="OldPage"
            ap:FrameNoHistory.Value="True" />
        <Frame
            x:Name="CurrentPage"
            ap:FrameNoHistory.Value="True" />
    </Grid>
</UserControl>
